<%= render 'creatives/navbar_sub' %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <div>
        <%= render 'users/user_info'%>
      </div>
      <div>
        <%= link_to 'edit', edit_user_registration_path %>
      </div>
    </section>

  </aside>
  <div class="col-md-8">
    <% if current_user.microposts.any? %>
      <ol class="microposts">
        <%= render @microposts %>
      </ol>
        <%= paginate @microposts %>
    <% end %>
  </div>
</div>